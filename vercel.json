{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "hello.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/hello",
        "dest": "hello.js"
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  